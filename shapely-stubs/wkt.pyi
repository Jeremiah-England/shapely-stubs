from typing import Protocol

from shapely.geometry.base import BaseGeometry

class _Readable(Protocol):
    def read(self): ...

class _Writeable(Protocol):
    def write(self): ...

def loads(data: str) -> BaseGeometry: ...
def load(fp: _Readable) -> BaseGeometry: ...
def dumps(ob, trim: bool = ..., **kw: int) -> str: ...
def dump(ob: BaseGeometry, fp: _Writeable, **settings: int | bool): ...
